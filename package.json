{
  "name": "canner-firebase-cms",
  "version": "1.0.0",
  "description": "Canner CMS for Firebase",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --history-api-fallback",
    "build": "webpack --mode production",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "import:auth": "firebase auth:import firebase-default-user.json",
    "serve": "firebase serve",
    "deploy": "firebase deploy --only hosting"
  },
  "theme": {
    "menu-dark-bg": "#373d62",
    "layout-sider-background": "#373d62",
    "layout-header-background": "#373d62",
    "primary-color": "#f2b173",
    "primary-3": "rgba(242, 177, 115, 0.5)",
    "primary-4": "rgba(242, 177, 115, 0.7)",
    "menu-dark-item-selected-bg": "#283050",
    "menu-dark-item-active-bg": "#283050",
    "item-active-bg": "rgba(242, 177, 115, 0.1)",
    "item-hover-bg": "rgba(242, 177, 115, 0.3)",
    "font-size-base": "14px"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "canner-schema-loader": "2.0.10",
    "canner-script": "2.0.10",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "prettier": "^1.15.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@canner/antd-components": "^0.5.10",
    "@canner/container": "^2.0.10",
    "@canner/history-router": "^2.0.10",
    "@canner/storage": "^2.0.10",
    "antd": "3.10.7",
    "canner": "^2.0.50",
    "canner-helpers": "^2.0.10",
    "canner-layouts": "^2.0.10",
    "firebase": "^5.5.8",
    "query-string": "^6.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-github-corner": "^2.3.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "slug": "^0.9.2",
    "styled-components": "^4.1.1"
  }
}
